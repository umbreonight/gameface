{
  "name": "gameface365-ui",
  "version": "0.0.1",
  "adonis-version": "4.1.0",
  "description": "Gameface365 UI Library in Adonis and React",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"node server.js\" \"npm run build\"",
    "dev": "concurrently --kill-others-on-fail \"adonis serve --dev\" \"npm run build-dev\"",
    "build-dev": "cross-env webpack --config webpack.config.js --watch --hide-modules --progress",
    "build": "cross-env webpack --config webpack.config.js --watch --progress"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@loadable/component": "^5.12.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^5.1.0",
    "pagination": "github:react-bootstrap/Pagination",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.12.0",
    "react-loader-spinner": "^3.1.5",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-slick": "^0.25.2",
    "react-tabs": "^3.1.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-stage-0": "^6.24.1",
    "autoprefixer": "^9.7.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
